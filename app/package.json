	"license": "(MIT OR GPL-2.0)",
	"keywords": [
		"Open Sound Control",
		"OSC",
		"Bitwig"
	],
	"dependencies": {
		"connect": "^3.7.0",
		"osc": "2.4.0",
		"serve-static": "^1.14.1",
		"ws": "7.2.1"
	}
